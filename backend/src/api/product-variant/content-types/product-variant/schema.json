{
  "kind": "collectionType",
  "collectionName": "product_variants",
  "info": {
    "singularName": "product-variant",
    "pluralName": "product-variants",
    "displayName": "ProductVariant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "SKU": {
      "type": "string"
    },
    "stock_quantity": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "product_option": {
      "type": "component",
      "repeatable": false,
      "component": "product-section.product-option"
    }
  }
}
