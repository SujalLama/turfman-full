{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string"
    },
    "area": {
      "type": "float"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "delivery": {
      "type": "enumeration",
      "enum": [
        "pickup",
        "deliver",
        "supply"
      ]
    },
    "contactMethod": {
      "type": "enumeration",
      "enum": [
        "phone",
        "email"
      ]
    },
    "message": {
      "type": "text"
    },
    "attachments": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
